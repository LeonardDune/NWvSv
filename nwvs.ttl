    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/4.4.18/lid/1/sublid/a>,
        <http://example.org/wet/strafvordering/artikel/4.4.18/lid/1/sublid/b>,
        <http://example.org/wet/strafvordering/artikel/4.4.18/lid/1/sublid/c>,
        <http://example.org/wet/strafvordering/artikel/4.4.18/lid/1/sublid/d>,
        <http://example.org/wet/strafvordering/artikel/4.4.18/lid/1/sublid/e> ;
