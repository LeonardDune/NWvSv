    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.11.3/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.11.3/lid/2>,
        <http://example.org/wet/strafvordering/artikel/1.11.3/lid/3> ;
    dcterms:identifier "artikel-1.11.3" .

<http://example.org/wet/strafvordering/artikel/1.11.9> a :Article ;
    rdfs:label "Artikel 1.11.9 (Titel 11.4 OPDRACHTEN AAN DE RECLASSERING)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.11.9/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.11.9/lid/2> ;
    dcterms:identifier "artikel-1.11.9" .

<http://example.org/wet/strafvordering/artikel/1.2.13/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 2.3 DE RAADKAMER; Artikel Artikel 1.2.13)"@nl ;
    dcterms:description "Indien een beslissing moet worden genomen tijdens de berechting, vindt de behandeling in raadkamer zoveel mogelijk plaats door de rechters die met de berechting van het desbetreffende strafbare feit zijn belast."@nl ;
    dcterms:identifier "artikel-1.2.13-lid-3" .

<http://example.org/wet/strafvordering/artikel/1.2.15/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 2.3 DE RAADKAMER; Artikel Artikel 1.2.15)"@nl ;
    dcterms:description "De betrokken procespartij en haar raadsman of advocaat kunnen van de op de zaak betrekking hebbende stukken kennisnemen, voor zover het belang van het onderzoek hierdoor niet ernstig wordt geschaad."@nl ;
    dcterms:identifier "artikel-1.2.15-lid-2" .

<http://example.org/wet/strafvordering/artikel/1.2.17> a :Article ;
    rdfs:label "Artikel 1.2.17 (Titel 2.3 DE RAADKAMER)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.2.17/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.2.17/lid/2> ;
    dcterms:identifier "artikel-1.2.17" .

<http://example.org/wet/strafvordering/artikel/1.2.18/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 2.3 DE RAADKAMER; Artikel Artikel 1.2.18)"@nl ;
    dcterms:description "De betrokken procespartij wordt gehoord, althans hiertoe opgeroepen. Artikel 1.6.1 is van overeenkomstige toepassing indien de betrokken procespartij niet de verdachte is."@nl ;
    dcterms:identifier "artikel-1.2.18-lid-2" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/1.6.1> .

<http://example.org/wet/strafvordering/artikel/1.2.18/lid/4> a :Paragraph ;
    rdfs:label "Lid 4 (Titel 2.3 DE RAADKAMER; Artikel Artikel 1.2.18)"@nl ;
    dcterms:description "Tenzij de wet anders bepaalt, kan de betrokken procespartij zijn belangen laten behartigen door een raadsman of advocaat indien die raadsman of advocaat verklaart daartoe uitdrukkelijk te zijn gemachtigd."@nl ;
    dcterms:identifier "artikel-1.2.18-lid-4" .

<http://example.org/wet/strafvordering/artikel/1.2.18/lid/5> a :Paragraph ;
    rdfs:label "Lid 5 (Titel 2.3 DE RAADKAMER; Artikel Artikel 1.2.18)"@nl ;
    dcterms:description "Het tweede tot en met het vierde lid zijn niet van toepassing voor zover het belang van het onderzoek hierdoor ernstig wordt geschaad."@nl ;
    dcterms:identifier "artikel-1.2.18-lid-5" .

<http://example.org/wet/strafvordering/artikel/1.2.21/lid/5> a :Paragraph ;
    rdfs:label "Lid 5 (Titel 2.3 DE RAADKAMER; Artikel Artikel 1.2.21)"@nl ;
    dcterms:description "Het proces-verbaal wordt door de voorzitter of een ander lid van de raadkamer en door de griffier ondertekend."@nl ;
    dcterms:identifier "artikel-1.2.21-lid-5" .

<http://example.org/wet/strafvordering/artikel/1.2.26/lid/4> a :Paragraph ;
    rdfs:label "Lid 4 (Titel 2.4 DE RECHTER-COMMISSARIS; Artikel Artikel 1.2.26)"@nl ;
    dcterms:description "De rechter-commissaris kan een bepaalde bevoegdheid ook binnen het rechtsgebied van een andere rechtbank uitoefenen. Hij stelt in dat geval zijn ambtgenoot hiervan tijdig in kennis."@nl ;
    dcterms:identifier "artikel-1.2.26-lid-4" .

<http://example.org/wet/strafvordering/artikel/1.2.31> a :Article ;
    rdfs:label "Artikel 1.2.31 (Titel 2.4 DE RECHTER-COMMISSARIS)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.2.31/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.2.31/lid/2>,
        <http://example.org/wet/strafvordering/artikel/1.2.31/lid/3> ;
    dcterms:identifier "artikel-1.2.31" .

<http://example.org/wet/strafvordering/artikel/1.2.6> a :Article ;
    rdfs:label "Artikel 1.2.6 (Titel 2.1 ALGEMENE BEPALINGEN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.2.6/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.2.6/lid/2>,
        <http://example.org/wet/strafvordering/artikel/1.2.6/lid/3> ;
    dcterms:identifier "artikel-1.2.6" .

<http://example.org/wet/strafvordering/artikel/1.2.6/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 2.1 ALGEMENE BEPALINGEN; Artikel Artikel 1.2.6)"@nl ;
    dcterms:description "Verwijzing op grond van het eerste of tweede lid kan ook voor de aanvang van het onderzoek op de zitting plaatsvinden door de voorzitter. Hij stelt de procespartijen daarvan in kennis en beveelt dat de officier van justitie van de verwijzing kennis geeft aan andere procesdeelnemers die al voor de terechtzitting zijn opgeroepen."@nl ;
    dcterms:identifier "artikel-1.2.6-lid-3" .

<http://example.org/wet/strafvordering/artikel/1.3.11/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 3.4 OPSPORING; Artikel Artikel 1.3.11)"@nl ;
    dcterms:description "Met de opsporing van strafbare feiten zijn als buitengewoon opsporingsambtenaar belast:"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.3.11/lid/1/sublid/a>,
        <http://example.org/wet/strafvordering/artikel/1.3.11/lid/1/sublid/b>,
        <http://example.org/wet/strafvordering/artikel/1.3.11/lid/1/sublid/c> ;
    dcterms:identifier "artikel-1.3.11-lid-1" .

<http://example.org/wet/strafvordering/artikel/1.3.14> a :Article ;
    rdfs:label "Artikel 1.3.14 (Titel 3.4 OPSPORING)"@nl ;
    dcterms:description "De officier van justitie kan de andere opsporingsambtenaren bevelen geven met betrekking tot de uitoefening van hun bevoegdheden."@nl ;
    dcterms:identifier "artikel-1.3.14" .

<http://example.org/wet/strafvordering/artikel/1.3.15> a :Article ;
    rdfs:label "Artikel 1.3.15 (Titel 3.4 OPSPORING)"@nl ;
    dcterms:description "De opsporingsambtenaar kan onder verantwoordelijkheid van de officier van justitie en met inachtneming van de door het College van procureurs-generaal gegeven algemene aanwijzingen van de uitoefening of de verdere uitoefening van zijn bevoegdheid tot opsporing afzien op gronden aan het algemeen belang ontleend."@nl ;
    dcterms:identifier "artikel-1.3.15" .

<http://example.org/wet/strafvordering/artikel/1.3.9> a :Article ;
    rdfs:label "Artikel 1.3.9 (Titel 3.3 LANDELIJKE BEVOEGDHEIDSUITOEFENING DOOR DE OFFICIER VAN JUSTITIE)"@nl ;
    dcterms:description "De officier van justitie die de bewaring vordert of die vordert dat de rechter-commissaris onderzoek verricht op grond van artikel 2.10.1, wendt zich bij de verdere behandeling van de zaak tot de rechtbank van hetzelfde arrondissement, tenzij nog geen procesinleiding is ingediend en hij van oordeel is dat: a. die rechtbank onbevoegd is het feit te berechten; b. de zaak moet worden samengevoegd met een zaak waarin bij een andere rechtbank onderzoek is of wordt verricht."@nl ;
    dcterms:identifier "artikel-1.3.9" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.10.1/sublid/a> .

<http://example.org/wet/strafvordering/artikel/1.4.1/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 4.1 DE VERDACHTE; Artikel Artikel 1.4.1)"@nl ;
    dcterms:description "Iemand mag alleen als verdachte worden aangemerkt indien ten aanzien van hem uit feiten of omstandigheden een redelijk vermoeden van schuld aan een strafbaar feit voortvloeit."@nl ;
    dcterms:identifier "artikel-1.4.1-lid-2" .

<http://example.org/wet/strafvordering/artikel/1.4.12/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 4.2 DE RAADSMAN; Afdeling 4.2.1 HET OPTREDEN VAN DE RAADSMAN; Artikel Artikel 1.4.12)"@nl ;
    dcterms:description "De verdachte kan een voorkeur voor een bepaalde raadsman kenbaar maken."@nl ;
    dcterms:identifier "artikel-1.4.12-lid-2" .

<http://example.org/wet/strafvordering/artikel/1.4.13> a :Article ;
    rdfs:label "Artikel 1.4.13 (Titel 4.2 DE RAADSMAN; Afdeling 4.2.1 HET OPTREDEN VAN DE RAADSMAN)"@nl ;
    dcterms:description "Voor de verdachte die geen raadsman heeft, wordt door het bestuur van de raad voor rechtsbijstand een raadsman aangewezen na kennisgeving door het openbaar ministerie dat: a. ten aanzien van hem de bewaring of gevangenneming is bevolen, dan wel, indien de verdachte niet in verzekering is gesteld, ten aanzien van hem de bewaring of gevangenneming is gevorderd; b. hoger beroep is ingesteld tegen het eindvonnis en het een zaak betreft waarin zijn voorlopige hechtenis is bevolen."@nl ;
    dcterms:identifier "artikel-1.4.13" .

<http://example.org/wet/strafvordering/artikel/1.4.18> a :Article ;
    rdfs:label "Artikel 1.4.18 (Titel 4.2 DE RAADSMAN; Afdeling 4.2.2 DE BEVOEGDHEDEN VAN DE RAADSMAN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.4.18/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.4.18/lid/2>,
        <http://example.org/wet/strafvordering/artikel/1.4.18/lid/3>,
        <http://example.org/wet/strafvordering/artikel/1.4.18/lid/4> ;
    dcterms:identifier "artikel-1.4.18" .

<http://example.org/wet/strafvordering/artikel/1.4.20/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 4.2 DE RAADSMAN; Afdeling 4.2.2 DE BEVOEGDHEDEN VAN DE RAADSMAN; Artikel Artikel 1.4.20)"@nl ;
    dcterms:description "Alle berichten die op grond van dit wetboek aan de verdachte ter kennis worden gebracht, worden ook aan zijn raadsman ter kennis gebracht."@nl ;
    dcterms:identifier "artikel-1.4.20-lid-2" .

<http://example.org/wet/strafvordering/artikel/1.4.3/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 4.1 DE VERDACHTE; Artikel Artikel 1.4.3)"@nl ;
    dcterms:description "De verdachte is niet verplicht tot antwoorden."@nl ;
    dcterms:identifier "artikel-1.4.3-lid-2" .

<http://example.org/wet/strafvordering/artikel/1.4.4> a :Article ;
    rdfs:label "Artikel 1.4.4 (Titel 4.1 DE VERDACHTE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.4.4/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.4.4/lid/2>,
        <http://example.org/wet/strafvordering/artikel/1.4.4/lid/3>,
        <http://example.org/wet/strafvordering/artikel/1.4.4/lid/4>,
        <http://example.org/wet/strafvordering/artikel/1.4.4/lid/5>,
        <http://example.org/wet/strafvordering/artikel/1.4.4/lid/6> ;
    dcterms:identifier "artikel-1.4.4" .

<http://example.org/wet/strafvordering/artikel/1.4.4/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 4.1 DE VERDACHTE; Artikel Artikel 1.4.4)"@nl ;
    dcterms:description "Aan de verdachte die niet is aangehouden, wordt voorafgaand aan zijn eerste verhoor, mededeling gedaan het bepaalde in artikel 1.4.3, tweede lid, van zijn recht op rechtsbijstand en, indien van toepassing, zijn recht op vertolking en vertaling."@nl ;
    dcterms:identifier "artikel-1.4.4-lid-2" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/1.4.3/lid/2> .

<http://example.org/wet/strafvordering/artikel/1.4.5/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 4.1 DE VERDACHTE; Artikel Artikel 1.4.5)"@nl ;
    dcterms:description "De verdachte heeft het recht om zich, overeenkomstig de bepalingen van dit wetboek, te doen bijstaan door een raadsman."@nl ;
    dcterms:identifier "artikel-1.4.5-lid-1" .

<http://example.org/wet/strafvordering/artikel/1.4.8> a :Article ;
    rdfs:label "Artikel 1.4.8 (Titel 4.1 DE VERDACHTE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.4.8/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.4.8/lid/2>,
        <http://example.org/wet/strafvordering/artikel/1.4.8/lid/3> ;
    dcterms:identifier "artikel-1.4.8" .

<http://example.org/wet/strafvordering/artikel/1.5.10/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 5.3 DE BENADEELDE PARTIJ; Artikel Artikel 1.5.10)"@nl ;
    dcterms:description "Indien het directe slachtoffer ten gevolge van het strafbare feit is overleden, kunnen zich voegen zijn erfgenamen ter zake van hun onder algemene titel verkregen vordering en de personen, bedoeld in artikel 108, eerste tot en met vierde lid, van Boek 6 van het Burgerlijk Wetboek ter zake van de daar bedoelde vorderingen. Indien het directe slachtoffer ten gevolge van het strafbare feit letsel heeft, kunnen zich voegen de personen, bedoeld in artikel 107, eerste lid, onderdelen a en b, van Boek 6 van het Burgerlijk Wetboek ter zake van de daar bedoelde vorderingen."@nl ;
    dcterms:identifier "artikel-1.5.10-lid-2" ;
    dcterms:references <http://example.org/wet/burgerlijk-wetboek/boek/6/artikel/107>,
        <http://example.org/wet/burgerlijk-wetboek/boek/6/artikel/108> .

<http://example.org/wet/strafvordering/artikel/1.5.13> a :Article ;
    rdfs:label "Artikel 1.5.13 (Titel 5.3 DE BENADEELDE PARTIJ)"@nl ;
    dcterms:description "Aan de benadeelde partij die de wens te kennen geeft dat zijn schade in het strafproces wordt vergoed, zendt de officier van justitie een door Onze Minister vastgesteld formulier toe voor de opgave van zijn vordering en de gronden waarop zij berust. Het formulier vermeldt het parket waarbij de benadeelde partij het formulier kan indienen."@nl ;
    dcterms:identifier "artikel-1.5.13" .

<http://example.org/wet/strafvordering/artikel/1.5.4> a :Article ;
    rdfs:label "Artikel 1.5.4 (Titel 5.2 DE RECHTEN VAN HET SLACHTOFFER)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.5.4/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.5.4/lid/2>,
        <http://example.org/wet/strafvordering/artikel/1.5.4/lid/3>,
        <http://example.org/wet/strafvordering/artikel/1.5.4/lid/4>,
        <http://example.org/wet/strafvordering/artikel/1.5.4/lid/5>,
        <http://example.org/wet/strafvordering/artikel/1.5.4/lid/6>,
        <http://example.org/wet/strafvordering/artikel/1.5.4/lid/7> ;
    dcterms:identifier "artikel-1.5.4" .

<http://example.org/wet/strafvordering/artikel/1.5.4/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 5.2 DE RECHTEN VAN HET SLACHTOFFER; Artikel Artikel 1.5.4)"@nl ;
    dcterms:description "Het slachtoffer dat daarom verzoekt wordt in kennis gesteld van informatie die hem in staat stelt te beslissen of hij beklag zal doen bij het gerechtshof als bedoeld in artikel 3.5.1. De kennisgeving van de informatie bedoeld in het eerste lid, onderdelen a en c, omvat naast de beslissing ten minste de motivering van de betrokken beslissing."@nl ;
    dcterms:identifier "artikel-1.5.4-lid-3" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/3.5.1/lid/1> .

<http://example.org/wet/strafvordering/artikel/1.5.5/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 5.2 DE RECHTEN VAN HET SLACHTOFFER; Artikel Artikel 1.5.5)"@nl ;
    dcterms:description "Het slachtoffer kan stukken die hij relevant acht voor de beoordeling van de zaak tegen de verdachte of van zijn vordering tegen de verdachte indienen ter voeging bij de processtukken. Artikel 1.8.4 is van overeen- komstige toepassing."@nl ;
    dcterms:identifier "artikel-1.5.5-lid-1" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/1.8.4> .

<http://example.org/wet/strafvordering/artikel/1.5.5/lid/5> a :Paragraph ;
    rdfs:label "Lid 5 (Titel 5.2 DE RECHTEN VAN HET SLACHTOFFER; Artikel Artikel 1.5.5)"@nl ;
    dcterms:description "Na het instellen van hoger beroep tegen het eindvonnis is het vierde lid van overeenkomstige toepassing."@nl ;
    dcterms:identifier "artikel-1.5.5-lid-5" .

<http://example.org/wet/strafvordering/artikel/1.5.6/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 5.2 DE RECHTEN VAN HET SLACHTOFFER; Artikel Artikel 1.5.6)"@nl ;
    dcterms:description "Het slachtoffer kan op de terechtzitting zijn belangen laten behartigen door een advocaat, indien deze verklaart daartoe uitdrukkelijk gemachtigd te zijn, of door een gemachtigde die daartoe een specifieke schriftelijke volmacht overlegt."@nl ;
    dcterms:identifier "artikel-1.5.6-lid-3" .

<http://example.org/wet/strafvordering/artikel/1.5.7/lid/6> a :Paragraph ;
    rdfs:label "Lid 6 (Titel 5.2 DE RECHTEN VAN HET SLACHTOFFER; Artikel Artikel 1.5.7)"@nl ;
    dcterms:description "Van een afwijzing van een verzoek als bedoeld in het eerste of derde lid, wordt het slachtoffer in kennis gesteld door degene aan wie dat verzoek is gericht."@nl ;
    dcterms:identifier "artikel-1.5.7-lid-6" .

<http://example.org/wet/strafvordering/artikel/1.5.8/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 5.2 DE RECHTEN VAN HET SLACHTOFFER; Artikel Artikel 1.5.8)"@nl ;
    dcterms:description "Het spreekrecht kan worden uitgeoefend indien het tenlastegelegde feit een misdrijf betreft waarop naar de wettelijke omschrijving gevange- nisstraf van acht jaar of meer is gesteld, dan wel een van de misdrijven betreft genoemd in de artikelen 240b, 247, 248a, 248b, 249, 250, 285, 285b, 300, tweede en derde lid, 301, tweede en derde lid, 306 tot en met 308, 318 en 372 van het Wetboek van Strafrecht en artikel 6 van de Wegenver- keerswet 1994. Van het voornemen tot het uitoefenen van het spreekrecht geven degenen die daartoe gerechtigd zijn, voor de aanvang van de terechtzitting kennis aan het openbaar ministerie opdat zij tijdig kunnen worden opgeroepen."@nl ;
    dcterms:identifier "artikel-1.5.8-lid-1" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/6> .

<http://example.org/wet/strafvordering/artikel/1.6.6> a :Article ;
    rdfs:label "Artikel 1.6.6 (Titel 6.2 VERSCHONINGSRECHTEN)"@nl ;
    dcterms:description "Getuigen kunnen zich van het beantwoorden van bepaalde vragen verschonen, indien zij daardoor: a. zichzelf, hun bloed- of aanverwant in de rechte lijn of in de zijlijn tot en met de derde graad dan wel hun echtgenoot, geregistreerde partner of levensgezel aan het gevaar van een strafrechtelijke vervolging zouden blootstellen; b. hun eerdere echtgenoot, hun eerdere geregistreerde partner of hun eerdere levensgezel aan het gevaar van een strafrechtelijke vervolging zouden blootstellen, tenzij de vragen betrekking hebben op feiten en omstandigheden die zich hebben voorgedaan nadat het huwelijk is ontbonden dan wel het geregistreerd partnerschap of partnerschap is beëindigd."@nl ;
    dcterms:identifier "artikel-1.6.6" .

<http://example.org/wet/strafvordering/artikel/1.8.3> a :Article ;
    rdfs:label "Artikel 1.8.3 (Titel 8.1 DE SAMENSTELLING VAN DE PROCESSTUKKEN EN BESLISSINGEN OVER VOEGING, KENNISNEMING, HET VERSTREKKEN VAN EEN KOPIE EN VERTALING)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.8.3/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.8.3/lid/2>,
        <http://example.org/wet/strafvordering/artikel/1.8.3/lid/3> ;
    dcterms:identifier "artikel-1.8.3" .

<http://example.org/wet/strafvordering/artikel/1.8.4> a :Article ;
    rdfs:label "Artikel 1.8.4 (Titel 8.1 DE SAMENSTELLING VAN DE PROCESSTUKKEN EN BESLISSINGEN OVER VOEGING, KENNISNEMING, HET VERSTREKKEN VAN EEN KOPIE EN VERTALING)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.8.4/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.8.4/lid/2>,
        <http://example.org/wet/strafvordering/artikel/1.8.4/lid/3> ;
    dcterms:identifier "artikel-1.8.4" .

<http://example.org/wet/strafvordering/artikel/1.8.6/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 8.1 DE SAMENSTELLING VAN DE PROCESSTUKKEN EN BESLISSINGEN OVER VOEGING, KENNISNEMING, HET VERSTREKKEN VAN EEN KOPIE EN VERTALING; Artikel Artikel 1.8.6)"@nl ;
    dcterms:description "Niettemin kan de officier van justitie, indien het belang van het onderzoek dit vereist, de verdachte de kennisneming van bepaalde processtukken onthouden. De verdachte wordt in dat geval ervan in kennis gesteld dat de hem ter kennisneming gegeven stukken niet volledig zijn."@nl ;
    dcterms:identifier "artikel-1.8.6-lid-2" .

<http://example.org/wet/strafvordering/artikel/1.8.7> a :Article ;
    rdfs:label "Artikel 1.8.7 (Titel 8.1 DE SAMENSTELLING VAN DE PROCESSTUKKEN EN BESLISSINGEN OVER VOEGING, KENNISNEMING, HET VERSTREKKEN VAN EEN KOPIE EN VERTALING)"@nl ;
    dcterms:description "Aan de verdachte mag niet de volledige kennisneming worden onthouden van: a. de processen-verbaal van zijn verhoren; b. de processen-verbaal van verhoren of handelingen van onderzoek, waarbij hij of zijn raadsman aanwezig kon zijn, tenzij en voor zover uit een proces-verbaal blijkt van een omstandigheid waarvan hij in het belang van het onderzoek tijdelijk onkundig moet blijven, en in verband daarmee een bevel als bedoeld in artikel 1.4.18, eerste lid, is gegeven; c. de processen-verbaal van verhoren waarvan hem de inhoud volledig of verkort is meegedeeld."@nl ;
    dcterms:identifier "artikel-1.8.7" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/1.4.18/sublid/c> .

<http://example.org/wet/strafvordering/artikel/1.8.9/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 8.1 DE SAMENSTELLING VAN DE PROCESSTUKKEN EN BESLISSINGEN OVER VOEGING, KENNISNEMING, HET VERSTREKKEN VAN EEN KOPIE EN VERTALING; Artikel Artikel 1.8.9)"@nl ;
    dcterms:description "De verdachte wordt van een afwijzing van zijn verzoek in kennis gesteld."@nl ;
    dcterms:identifier "artikel-1.8.9-lid-2" .

<http://example.org/wet/strafvordering/artikel/1.9.12/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 9.2 BETEKENING; Artikel Artikel 1.9.12)"@nl ;
    dcterms:description "Het openbaar ministerie kan de uitreiking opdragen aan een ambtenaar van politie, aangesteld voor de uitvoering van de politietaak, dan wel aan een andere ambtenaar of functionaris die daartoe bij ministeriële regeling is aangewezen."@nl ;
    dcterms:identifier "artikel-1.9.12-lid-2" .

<http://example.org/wet/strafvordering/artikel/1.9.13/lid/6> a :Paragraph ;
    rdfs:label "Lid 6 (Titel 9.2 BETEKENING; Artikel Artikel 1.9.13)"@nl ;
    dcterms:description "Indien het bericht bestemd is voor een rechtspersoon, worden de woonplaats van de rechtspersoon, de plaats van het kantoor van de rechtspersoon en de woonplaats van elk van de bestuurders als woon- of verblijfplaats in de zin van het vijfde lid aangemerkt. Indien het bericht bestemd is voor een maatschap of vennootschap zonder rechtspersoon- lijkheid worden de plaats van het kantoor van de maatschap of vennoot- schap en de woonplaats van elk van de aansprakelijke vennoten als woon- of verblijfplaats aangemerkt. De vorige zin is van overeenkomstige toepassing bij een doelvermogen en haar bestuurders en boekhouder en ook bij een rederij en haar leden."@nl ;
    dcterms:identifier "artikel-1.9.13-lid-6" .

<http://example.org/wet/strafvordering/artikel/1.9.13/lid/8> a :Paragraph ;
    rdfs:label "Lid 8 (Titel 9.2 BETEKENING; Artikel Artikel 1.9.13)"@nl ;
    dcterms:description "De weigering van degene voor wie het bericht bestemd is om het bericht in ontvangst te nemen, wordt gelijkgesteld met een uitreiking aan die persoon. Deze regel is bij het derde en vierde lid van overeenkomstige toepassing."@nl ;
    dcterms:identifier "artikel-1.9.13-lid-8" .

<http://example.org/wet/strafvordering/artikel/2.1.10/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 1.4 VERSLAGLEGGING DOOR OPSPORINGSAMBTENAREN; Artikel Artikel 2.1.10)"@nl ;
    dcterms:description "Opsporingsambtenaren kunnen het opmaken van proces-verbaal uitstellen indien op een geluids- of beeldopname is vastgelegd wat door hen tot opsporing is verricht of bevonden."@nl ;
    dcterms:identifier "artikel-2.1.10-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.1.14/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 1.5 VASTLEGGING VAN BEVELEN EN VAN MACHTIGINGEN EN VORDERINGEN DAARTOE; Artikel Artikel 2.1.14)"@nl ;
    dcterms:description "Een machtiging van de rechter-commissaris wordt gegeven op vordering van de officier van justitie."@nl ;
    dcterms:identifier "artikel-2.1.14-lid-1" .

<http://example.org/wet/strafvordering/artikel/2.1.15> a :Article ;
    rdfs:label "Artikel 2.1.15 (Titel 1.5 VASTLEGGING VAN BEVELEN EN VAN MACHTIGINGEN EN VORDERINGEN DAARTOE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.1.15/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.1.15/lid/2> ;
    dcterms:identifier "artikel-2.1.15" .

<http://example.org/wet/strafvordering/artikel/2.1.17> a :Article ;
    rdfs:label "Artikel 2.1.17 (Titel 1.6 TOESTEMMING VOOR ONDERZOEKSHANDELINGEN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.1.17/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.1.17/lid/2> ;
    dcterms:identifier "artikel-2.1.17" .

<http://example.org/wet/strafvordering/artikel/2.1.2/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 1.2 ALGEMENE BEPALINGEN OVER DE UITOEFENING VAN BEVOEGDHEDEN IN HET KADER VAN HET OPSPORINGSON- DERZOEK; Artikel Artikel 2.1.2)"@nl ;
    dcterms:description "Onverminderd nadere beperking bij of krachtens de wet wordt een bevoegdheid alleen uitgeoefend indien dit in het belang van het onderzoek is."@nl ;
    dcterms:identifier "artikel-2.1.2-lid-2" .

<http://example.org/wet/strafvordering/artikel/2.1.4> a :Article ;
    rdfs:label "Artikel 2.1.4 (Titel 1.2 ALGEMENE BEPALINGEN OVER DE UITOEFENING VAN BEVOEGDHEDEN IN HET KADER VAN HET OPSPORINGSON- DERZOEK)"@nl ;
    dcterms:description "Bij de uitoefening van een bevoegdheid brengt de opsporings- ambtenaar een persoon niet tot andere strafbare feiten dan die waarop zijn opzet al tevoren was gericht."@nl ;
    dcterms:identifier "artikel-2.1.4" .

<http://example.org/wet/strafvordering/artikel/2.1.7/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 1.2 ALGEMENE BEPALINGEN OVER DE UITOEFENING VAN BEVOEGDHEDEN IN HET KADER VAN HET OPSPORINGSON- DERZOEK; Artikel Artikel 2.1.7)"@nl,
        "Lid 3 (Titel 1.2 ALGEMENE BEPALINGEN OVER DE UITOEFENING VAN BEVOEGDHEDEN IN HET KADER VAN HET OPSPORINGSON- DERZOEK; Artikel Artikel 2.1.7a)"@nl ;
    dcterms:description "Artikel 2.1.7, tweede en derde lid, is van overeenkomstige toepassing."@nl,
        "De termijn voor het instellen van beroep is twee weken na de dagtekening van de beslissing. De rechtbank beslist zo spoedig mogelijk."@nl ;
    dcterms:identifier "artikel-2.1.7-lid-3" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.1.7/lid/3> .

<http://example.org/wet/strafvordering/artikel/2.10.10> a :Article ;
    rdfs:label "Artikel 2.10.10 (Titel 10.1 ALGEMENE BEPALINGEN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.10/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.10.10/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.10.10/lid/3>,
        <http://example.org/wet/strafvordering/artikel/2.10.10/lid/4>,
        <http://example.org/wet/strafvordering/artikel/2.10.10/lid/5>,
        <http://example.org/wet/strafvordering/artikel/2.10.10/lid/6>,
        <http://example.org/wet/strafvordering/artikel/2.10.10/lid/7> ;
    dcterms:identifier "artikel-2.10.10" .

<http://example.org/wet/strafvordering/artikel/2.10.10/lid/5> a :Paragraph ;
    rdfs:label "Lid 5 (Titel 10.1 ALGEMENE BEPALINGEN; Artikel Artikel 2.10.10)"@nl ;
    dcterms:description "De verdachte, de getuige of de deskundige ondertekent zijn verklaring nadat deze hem is voorgelezen of door hem is gelezen, indien hij met de verklaring instemt. Indien ondertekening achterwege blijft, wordt de weigering of oorzaak daarvan vermeld."@nl ;
    dcterms:identifier "artikel-2.10.10-lid-5" .

<http://example.org/wet/strafvordering/artikel/2.10.14> a :Article ;
    rdfs:label "Artikel 2.10.14 (Titel 10.2 BEVOEGDHEDEN MET BETREKKING TOT DE VERDACHTE; Afdeling 10.2.1 HET VERHOOR VAN DE VERDACHTE)"@nl ;
    dcterms:description "Indien de verdachte in vrijheid is en op een betekende oproeping niet verschijnt, kan de rechter-commissaris hem opnieuw oproepen en daarbij of nadien een bevel tot medebrenging geven."@nl ;
    dcterms:identifier "artikel-2.10.14" .

<http://example.org/wet/strafvordering/artikel/2.10.18/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 10.2 BEVOEGDHEDEN MET BETREKKING TOT DE VERDACHTE; Afdeling 10.2.1 HET VERHOOR VAN DE VERDACHTE; Artikel Artikel 2.10.18)"@nl ;
    dcterms:description "De officier van justitie kan de vragen opgeven die hij gesteld wenst te zien. De rechter-commissaris stelt op een door hem geschikt geacht moment de officier van justitie in de gelegenheid vragen te stellen. De rechter-commissaris kan bepalen dat vragen door zijn tussenkomst worden gesteld."@nl ;
    dcterms:identifier "artikel-2.10.18-lid-2" .

<http://example.org/wet/strafvordering/artikel/2.10.2/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 10.1 ALGEMENE BEPALINGEN; Artikel Artikel 2.10.2)"@nl ;
    dcterms:description "De verdachte die is verhoord kan de rechter-commissaris verzoeken onderzoek te verrichten als bedoeld in artikel 2.10.1, eerste lid. Het verzoek vermeldt het strafbare feit waarop het onderzoek betrekking dient te hebben en het gewenste onderzoek."@nl ;
    dcterms:identifier "artikel-2.10.2-lid-1" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.10.1/lid/1> .

<http://example.org/wet/strafvordering/artikel/2.10.2/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 10.1 ALGEMENE BEPALINGEN; Artikel Artikel 2.10.2)"@nl ;
    dcterms:description "De rechter-commissaris kan de verdachte horen over het verzoek. De verdachte kan zich daarbij door een raadsman doen bijstaan. De rechter- commissaris stelt de officier van justitie in kennis van de tijd en plaats van het horen. De officier van justitie kan bij het horen aanwezig zijn en de nodige opmerkingen maken."@nl ;
    dcterms:identifier "artikel-2.10.2-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.10.24> a :Article ;
    rdfs:label "Artikel 2.10.24 (Titel 10.2 BEVOEGDHEDEN MET BETREKKING TOT DE VERDACHTE; Afdeling 10.2.2 OBSERVATIE VAN DE VERDACHTE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.24/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.10.24/lid/2> ;
    dcterms:identifier "artikel-2.10.24" .

<http://example.org/wet/strafvordering/artikel/2.10.25> a :Article ;
    rdfs:label "Artikel 2.10.25 (Titel 10.2 BEVOEGDHEDEN MET BETREKKING TOT DE VERDACHTE; Afdeling 10.2.2 OBSERVATIE VAN DE VERDACHTE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.25/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.10.25/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.10.25/lid/3>,
        <http://example.org/wet/strafvordering/artikel/2.10.25/lid/4> ;
    dcterms:identifier "artikel-2.10.25" .

<http://example.org/wet/strafvordering/artikel/2.10.25/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 10.2 BEVOEGDHEDEN MET BETREKKING TOT DE VERDACHTE; Afdeling 10.2.2 OBSERVATIE VAN DE VERDACHTE; Artikel Artikel 2.10.25)"@nl ;
    dcterms:description "De rechter-commissaris kan ambtshalve, op vordering van de officier van justitie of op verzoek van de verdachte het bevel eenmaal met ten hoogste zeven weken verlengen. Op het bevel tot verlenging is artikel 2.10.24 van overeenkomstige toepassing, met dien verstande dat het oordeel van een of meer deskundigen achterwege kan blijven."@nl ;
    dcterms:identifier "artikel-2.10.25-lid-2" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.10.24> .

<http://example.org/wet/strafvordering/artikel/2.10.25/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 10.2 BEVOEGDHEDEN MET BETREKKING TOT DE VERDACHTE; Afdeling 10.2.2 OBSERVATIE VAN DE VERDACHTE; Artikel Artikel 2.10.25)"@nl ;
    dcterms:description "De rechter-commissaris kan ambtshalve, op vordering van de officier van justitie of op verzoek van de verdachte bevelen dat het verblijf in de accommodatie of instelling wordt beëindigd."@nl ;
    dcterms:identifier "artikel-2.10.25-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.10.28/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 10.3 HET VERHOOR VAN DE GETUIGE; Afdeling 10.3.1 ALGEMENE BEPALINGEN; Artikel Artikel 2.10.28)"@nl ;
    dcterms:description "De raadsman kan de vragen opgeven die hij gesteld wenst te zien. De rechter-commissaris stelt op een door hem geschikt geacht moment de raadsman in de gelegenheid vragen te stellen. Indien de getuige tijdens het opsporingsonderzoek nog niet is verhoord en op verzoek van de verdachte wordt verhoord, krijgt de raadsman als eerste de gelegenheid om vragen te stellen. De rechter-commissaris kan bepalen dat vragen door zijn tussenkomst worden gesteld."@nl ;
    dcterms:identifier "artikel-2.10.28-lid-2" .

<http://example.org/wet/strafvordering/artikel/2.10.29/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 10.3 HET VERHOOR VAN DE GETUIGE; Afdeling 10.3.1 ALGEMENE BEPALINGEN; Artikel Artikel 2.10.29)"@nl ;
    dcterms:description "De rechter-commissaris kan, indien hij dit in het belang van het onderzoek wenselijk acht, de verdachte ambtshalve of op zijn verzoek in de gelegenheid stellen het verhoor van de getuige bij te wonen."@nl ;
    dcterms:identifier "artikel-2.10.29-lid-1" .

<http://example.org/wet/strafvordering/artikel/2.10.30/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 10.3 HET VERHOOR VAN DE GETUIGE; Afdeling 10.3.1 ALGEMENE BEPALINGEN; Artikel Artikel 2.10.30)"@nl ;
    dcterms:description "Indien een getuige met een psychische stoornis, psychogeriatrische aandoening of verstandelijke handicap naar het oordeel van de rechter- commissaris de betekenis van de eed niet voldoende beseft, of indien de getuige de leeftijd van zestien jaar nog niet heeft bereikt, wordt hij niet beëdigd, maar aangemaand de gehele waarheid en niets dan de waarheid te zeggen."@nl ;
    dcterms:identifier "artikel-2.10.30-lid-2" .

<http://example.org/wet/strafvordering/artikel/2.10.39> a :Article ;
    rdfs:label "Artikel 2.10.39 (Titel 10.3 HET VERHOOR VAN DE GETUIGE; Afdeling 10.3.3 BEDREIGDE GETUIGEN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.39/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.10.39/lid/2> ;
    dcterms:identifier "artikel-2.10.39" .

<http://example.org/wet/strafvordering/artikel/2.10.42/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 10.3 HET VERHOOR VAN DE GETUIGE; Afdeling 10.3.3 BEDREIGDE GETUIGEN; Artikel Artikel 2.10.42)"@nl ;
    dcterms:description "In het geval dat de rechter-commissaris belet dat een door de bedreigde getuige gegeven antwoord ter kennis komt van de officier van justitie, de verdachte of zijn raadsman, doet de rechter-commissaris in het proces-verbaal opnemen dat de gestelde vraag door de bedreigde getuige is beantwoord."@nl ;
    dcterms:identifier "artikel-2.10.42-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.10.44> a :Article ;
    rdfs:label "Artikel 2.10.44 (Titel 10.3 HET VERHOOR VAN DE GETUIGE; Afdeling 10.3.3 BEDREIGDE GETUIGEN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.44/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.10.44/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.10.44/lid/3> ;
    dcterms:identifier "artikel-2.10.44" .

<http://example.org/wet/strafvordering/artikel/2.10.44/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 10.3 HET VERHOOR VAN DE GETUIGE; Afdeling 10.3.3 BEDREIGDE GETUIGEN; Artikel Artikel 2.10.44)"@nl ;
    dcterms:description "De anonimisering wordt door de rechter-commissaris en de griffier ondertekend of gewaarmerkt."@nl ;
    dcterms:identifier "artikel-2.10.44-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.10.45> a :Article ;
    rdfs:label "Artikel 2.10.45 (Titel 10.3 HET VERHOOR VAN DE GETUIGE; Afdeling 10.3.4 AFGESCHERMDE GETUIGEN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.45/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.10.45/lid/2> ;
    dcterms:identifier "artikel-2.10.45" .

<http://example.org/wet/strafvordering/artikel/2.10.47/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 10.3 HET VERHOOR VAN DE GETUIGE; Afdeling 10.3.4 AFGESCHERMDE GETUIGEN; Artikel Artikel 2.10.47)"@nl ;
    dcterms:description "De rechter-commissaris brengt, indien de getuige daarmee instemt, het proces-verbaal ter kennis van de officier van justitie, de verdachte en zijn raadsman. De getuige kan zijn instemming alleen onthouden indien het belang van de staatsveiligheid dit vereist. In geval de getuige zijn instemming onthoudt, draagt de rechter-commissaris er zorg voor dat het proces-verbaal van verhoor en alle andere gegevens met betrekking tot het verhoor direct worden vernietigd. De rechter-commissaris maakt hiervan proces-verbaal op."@nl ;
    dcterms:identifier "artikel-2.10.47-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.10.52/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 10.4 DE BENOEMING EN HET VERHOOR VAN EEN DESKUNDIGE; Afdeling 10.4.1 DE BENOEMING VAN EEN DESKUNDIGE; Artikel Artikel 2.10.52)"@nl ;
    dcterms:description "Tenzij het belang van het onderzoek zich daartegen verzet, kiest de rechter-commissaris een of meer van de deskundigen uit de voorge- dragen personen."@nl ;
    dcterms:identifier "artikel-2.10.52-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.10.53> a :Article ;
    rdfs:label "Artikel 2.10.53 (Titel 10.4 DE BENOEMING EN HET VERHOOR VAN EEN DESKUNDIGE; Afdeling 10.4.1 DE BENOEMING VAN EEN DESKUNDIGE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.53/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.10.53/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.10.53/lid/3>,
        <http://example.org/wet/strafvordering/artikel/2.10.53/lid/4>,
        <http://example.org/wet/strafvordering/artikel/2.10.53/lid/5> ;
    dcterms:identifier "artikel-2.10.53" .

<http://example.org/wet/strafvordering/artikel/2.10.59> a :Article ;
    rdfs:label "Artikel 2.10.59 (Titel 10.5 GETUIGEN AAN WIE TOEZEGGINGEN ZIJN GEDAAN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.59/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.10.59/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.10.59/lid/3>,
        <http://example.org/wet/strafvordering/artikel/2.10.59/lid/4> ;
    dcterms:identifier "artikel-2.10.59" .

<http://example.org/wet/strafvordering/artikel/2.10.60/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 10.5 GETUIGEN AAN WIE TOEZEGGINGEN ZIJN GEDAAN; Artikel Artikel 2.10.60)"@nl ;
    dcterms:description "De rechter-commissaris beoordeelt de rechtmatigheid van de afspraak. Hij houdt daarbij rekening met de dringende noodzaak en met het belang van het verkrijgen van de door de getuige af te leggen verklaring. Hij geeft tevens een oordeel over de betrouwbaarheid van de getuige. Hij legt zijn oordeel neer in een beslissing. Indien hij de afspraak rechtmatig oordeelt, komt deze tot stand."@nl ;
    dcterms:identifier "artikel-2.10.60-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.10.64> a :Article ;
    rdfs:label "Artikel 2.10.64 (Titel 10.6 VOORTGANGSBEWAKING DOOR DE RECHTER- COMMISSARIS)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.64/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.10.64/lid/2> ;
    dcterms:identifier "artikel-2.10.64" .

<http://example.org/wet/strafvordering/artikel/2.10.65> a :Article ;
    rdfs:label "Artikel 2.10.65 (Titel 10.6 VOORTGANGSBEWAKING DOOR DE RECHTER- COMMISSARIS)"@nl ;
    dcterms:description "De rechter-commissaris kan na ontvangst van de kennisgeving, bedoeld in artikel 2.10.64, en indien hij op grond van de artikelen 2.10.1 tot en met"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.65/lid/2> ;
    dcterms:identifier "artikel-2.10.65" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.10.64> .

<http://example.org/wet/strafvordering/artikel/2.10.67/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 10.6 VOORTGANGSBEWAKING DOOR DE RECHTER- COMMISSARIS; Artikel Artikel 2.10.67)"@nl ;
    dcterms:description "In het geval de rechter-commissaris de voortgang van het opspo- ringsonderzoek beoordeelt kan hij de officier van justitie een termijn stellen voor het nemen van een vervolgingsbeslissing. De rechter- commissaris kan de zaak tevens voorleggen aan de rechtbank, met het oog op toepassing van artikel 3.1.3."@nl ;
    dcterms:identifier "artikel-2.10.67-lid-2" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/3.1.3> .

<http://example.org/wet/strafvordering/artikel/2.10.68> a :Article ;
    rdfs:label "Artikel 2.10.68 (Titel 10.7 BEËINDIGING VAN HET ONDERZOEK)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.10.68/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.10.68/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.10.68/lid/3> ;
    dcterms:identifier "artikel-2.10.68" .

<http://example.org/wet/strafvordering/artikel/2.10.70/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 10.8 RECHTSMIDDELEN; Artikel Artikel 2.10.70)"@nl ;
    dcterms:description "Het beroep wordt behandeld door de rechtbank. Het beroep tegen de beslissingen, bedoeld in het tweede lid, wordt behandeld door het gerecht in feitelijke aanleg dat de zaak berecht of zal berechten."@nl ;
    dcterms:identifier "artikel-2.10.70-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.2.1/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 2.1 ALGEMENE BEPALINGEN; Artikel Artikel 2.2.1)"@nl ;
    dcterms:description "Degene die een klacht heeft ingediend, kan het verzoek tot vervolging binnen een week na de indiening bij een opsporingsambtenaar intrekken."@nl ;
    dcterms:identifier "artikel-2.2.1-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.3.3/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 3.1 ALGEMENE BEPALINGEN; Artikel Artikel 2.3.3)"@nl ;
    dcterms:description "Het proces-verbaal van verhoor vermeldt het tijdstip waarop het verhoor is aangevangen, eventueel wordt onderbroken en hervat, en waarop het is beëindigd. Het bevat de redenen voor het onderbreken van het verhoor; het vermeldt verder de locatie van het verhoor en de identiteit van de personen die aan het verhoor hebben deelgenomen. Aangetekend wordt of van het verhoor geluidsopnamen of geluids- en beeldopnamen zijn gemaakt."@nl ;
    dcterms:identifier "artikel-2.3.3-lid-1" .

<http://example.org/wet/strafvordering/artikel/2.3.7/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 3.2 HET VERHOOR VAN DE VERDACHTE; Artikel Artikel 2.3.7)"@nl ;
    dcterms:description "Indien een verdachte die is aangehouden voor een misdrijf waarop naar de wettelijke omschrijving gevangenisstraf van vier jaar of meer is gesteld, desgevraagd rechtsbijstand wenst, stelt de officier van justitie of de hulpofficier van justitie die bij de voorgeleiding beveelt dat de verdachte wordt opgehouden voor onderzoek, het bestuur van de raad voor rechtsbijstand hiervan direct in kennis, opdat het bestuur een raadsman aanwijst. De tweede zin van het eerste lid is van overeenkom- stige toepassing."@nl ;
    dcterms:identifier "artikel-2.3.7-lid-2" .

<http://example.org/wet/strafvordering/artikel/2.3.8/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 3.2 HET VERHOOR VAN DE VERDACHTE; Artikel Artikel 2.3.8)"@nl ;
    dcterms:description "De aangehouden verdachte voor wie ingevolge artikel 2.3.7 een raadsman beschikbaar is, wordt de gelegenheid verschaft om vooraf- gaand aan het eerste verhoor gedurende ten hoogste een half uur met hem een onderhoud te hebben. De officier van justitie of de hulpofficier van justitie kan deze termijn, indien deze ontoereikend blijkt, op verzoek van de verdachte of zijn raadsman met ten hoogste een half uur verlengen, tenzij het belang van het onderzoek zich daartegen verzet. Het onderhoud kan ook door middel van telecommunicatie plaatsvinden."@nl ;
    dcterms:identifier "artikel-2.3.8-lid-1" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.3.7> .

<http://example.org/wet/strafvordering/artikel/2.4.1/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 3.3 HET VERHOOR VAN DE GETUIGE; Hoofdstuk 4 DESKUNDIGENONDERZOEK IN OPDRACHT VAN DE OFFICIER VAN JUSTITIE; Artikel Artikel 2.4.1)"@nl ;
    dcterms:description "Bij een verzoek tot benoeming van een deskundige kan de verdachte een of meer personen ter benoeming voordragen."@nl ;
    dcterms:identifier "artikel-2.4.1-lid-3" .

<http://example.org/wet/strafvordering/artikel/2.4.1/lid/5> a :Paragraph ;
    rdfs:label "Lid 5 (Titel 3.3 HET VERHOOR VAN DE GETUIGE; Hoofdstuk 4 DESKUNDIGENONDERZOEK IN OPDRACHT VAN DE OFFICIER VAN JUSTITIE; Artikel Artikel 2.4.1)"@nl ;
    dcterms:description "De officier van justitie wijst een verzoek van de verdachte tot benoeming van een deskundige toe voor zover dit redelijkerwijs van belang kan zijn voor de in het kader van de berechting door de rechter te nemen beslissingen. Hij geeft de verdachte direct kennis van zijn beslissing. Een afwijzing wordt gemotiveerd."@nl ;
    dcterms:identifier "artikel-2.4.1-lid-5" .

<http://example.org/wet/strafvordering/artikel/2.4.2> a :Article ;
    rdfs:label "Artikel 2.4.2 (Titel 3.3 HET VERHOOR VAN DE GETUIGE; Hoofdstuk 4 DESKUNDIGENONDERZOEK IN OPDRACHT VAN DE OFFICIER VAN JUSTITIE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.4.2/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.4.2/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.4.2/lid/3>,
        <http://example.org/wet/strafvordering/artikel/2.4.2/lid/4>,
        <http://example.org/wet/strafvordering/artikel/2.4.2/lid/5> ;
    dcterms:identifier "artikel-2.4.2" .

<http://example.org/wet/strafvordering/artikel/2.5.12> a :Article ;
    rdfs:label "Artikel 2.5.12 (Titel 5.3 OPHOUDEN VOOR ONDERZOEK EN INVERZEKERING- STELLING; Afdeling 5.3.1 OPHOUDEN VOOR ONDERZOEK)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.12/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.5.12/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.5.12/lid/3>,
        <http://example.org/wet/strafvordering/artikel/2.5.12/lid/4> ;
    dcterms:identifier "artikel-2.5.12" .

<http://example.org/wet/strafvordering/artikel/2.5.12/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 5.3 OPHOUDEN VOOR ONDERZOEK EN INVERZEKERING- STELLING; Afdeling 5.3.1 OPHOUDEN VOOR ONDERZOEK; Artikel Artikel 2.5.12)"@nl ;
    dcterms:description "Op verzoek van de aangehouden verdachte doet de officier van justitie of de hulpofficier van justitie die bij de voorgeleiding beveelt dat de verdachte wordt opgehouden voor onderzoek, direct mededeling van zijn vrijheidsbeneming aan ten minste een door de verdachte aangeduide persoon."@nl ;
    dcterms:identifier "artikel-2.5.12-lid-1" .

<http://example.org/wet/strafvordering/artikel/2.5.12/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 5.3 OPHOUDEN VOOR ONDERZOEK EN INVERZEKERING- STELLING; Afdeling 5.3.1 OPHOUDEN VOOR ONDERZOEK; Artikel Artikel 2.5.12)"@nl ;
    dcterms:description "Op verzoek van de aangehouden verdachte die niet de Nederlandse nationaliteit heeft, doet de officier van justitie of de hulpofficier van justitie die bij de voorgeleiding beslist om de verdachte op te houden voor onderzoek, direct mededeling van zijn vrijheidsbeneming aan de consulaire post van de staat waarvan de verdachte de nationaliteit heeft."@nl ;
    dcterms:identifier "artikel-2.5.12-lid-2" .

<http://example.org/wet/strafvordering/artikel/2.5.13> a :Article ;
    rdfs:label "Artikel 2.5.13 (Titel 5.3 OPHOUDEN VOOR ONDERZOEK EN INVERZEKERING- STELLING; Afdeling 5.3.2 INVERZEKERINGSTELLING)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.13/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.5.13/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.5.13/lid/3> ;
    dcterms:identifier "artikel-2.5.13" .

<http://example.org/wet/strafvordering/artikel/2.5.16> a :Article ;
    rdfs:label "Artikel 2.5.16 (Titel 5.3 OPHOUDEN VOOR ONDERZOEK EN INVERZEKERING- STELLING; Afdeling 5.3.3 MAATREGELEN TEN AANZIEN VAN DE VOOR ONDERZOEK OPGEHOUDEN OF IN VERZEKERING GESTELDE VERDACHTE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.16/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.5.16/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.5.16/lid/3> ;
    dcterms:identifier "artikel-2.5.16" .

<http://example.org/wet/strafvordering/artikel/2.5.16/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 5.3 OPHOUDEN VOOR ONDERZOEK EN INVERZEKERING- STELLING; Afdeling 5.3.3 MAATREGELEN TEN AANZIEN VAN DE VOOR ONDERZOEK OPGEHOUDEN OF IN VERZEKERING GESTELDE VERDACHTE; Artikel Artikel 2.5.16)"@nl ;
    dcterms:description "Onverminderd de artikelen 1.4.17 en 1.4.18 kan de officier van justitie of de hulpofficier van justitie bevelen dat de verdachte aan de volgende maatregelen wordt onderworpen:"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.16/lid/1/sublid/a>,
        <http://example.org/wet/strafvordering/artikel/2.5.16/lid/1/sublid/b> ;
    dcterms:identifier "artikel-2.5.16-lid-1" .

<http://example.org/wet/strafvordering/artikel/2.5.17/lid/5> a :Paragraph ;
    rdfs:label "Lid 5 (Titel 5.3 OPHOUDEN VOOR ONDERZOEK EN INVERZEKERING- STELLING; Afdeling 5.3.4 VOORGELEIDING AAN DE RECHTER-COMMISSARIS; Artikel Artikel 2.5.17)"@nl ;
    dcterms:description "Indien de rechter-commissaris de voortzetting van de inverzekering- stelling onrechtmatig oordeelt, beveelt hij de directe invrijheidstelling van de verdachte. Indien hij de voortzetting van de inverzekeringstelling alleen voor een beperkte tijd rechtmatig oordeelt, beveelt hij dat de verdachte binnen een bepaalde termijn in vrijheid wordt gesteld. Het bevel is dadelijk uitvoerbaar."@nl ;
    dcterms:identifier "artikel-2.5.17-lid-5" .

<http://example.org/wet/strafvordering/artikel/2.5.21/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.1 ALGEMENE BEPALINGEN; Artikel Artikel 2.5.21)"@nl ;
    dcterms:description "Het bevel omschrijft zo nauwkeurig mogelijk het strafbare feit ten aanzien waarvan de verdenking is gerezen, de feiten of omstandigheden waarop de ernstige bezwaren tegen de verdachte zijn gegrond en de gedragingen, feiten of omstandigheden, waaronder de persoonlijke omstandigheden van de verdachte, waaruit blijkt op welke van de gronden, bedoeld in artikel 2.5.27, eerste lid, het bevel is gebaseerd en die het bevel noodzakelijk maken."@nl ;
    dcterms:identifier "artikel-2.5.21-lid-2" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.5.27/lid/1> .

<http://example.org/wet/strafvordering/artikel/2.5.23/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.1 ALGEMENE BEPALINGEN; Artikel Artikel 2.5.23)"@nl ;
    dcterms:description "Van het verhoor wordt, ook indien dit door een daartoe aangewezen lid van de rechtbank of van het gerechtshof wordt afgenomen, met overeenkomstige toepassing van artikel 2.10.10 proces-verbaal opgemaakt."@nl ;
    dcterms:identifier "artikel-2.5.23-lid-3" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.10.10> .

<http://example.org/wet/strafvordering/artikel/2.5.27> a :Article ;
    rdfs:label "Artikel 2.5.27 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.2 TOEPASSINGSVOORWAARDEN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.27/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.5.27/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.5.27/lid/3> ;
    dcterms:identifier "artikel-2.5.27" .

<http://example.org/wet/strafvordering/artikel/2.5.33> a :Article ;
    rdfs:label "Artikel 2.5.33 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.3 SCHORSING VAN DE VOORLOPIGE HECHTENIS)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.33/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.5.33/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.5.33/lid/3>,
        <http://example.org/wet/strafvordering/artikel/2.5.33/lid/4> ;
    dcterms:identifier "artikel-2.5.33" .

<http://example.org/wet/strafvordering/artikel/2.5.33/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.3 SCHORSING VAN DE VOORLOPIGE HECHTENIS; Artikel Artikel 2.5.33)"@nl ;
    dcterms:description "Aan de schorsing van de voorlopige hechtenis zijn van rechtswege de voorwaarden verbonden dat:"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.33/lid/1/sublid/a>,
        <http://example.org/wet/strafvordering/artikel/2.5.33/lid/1/sublid/b>,
        <http://example.org/wet/strafvordering/artikel/2.5.33/lid/1/sublid/c>,
        <http://example.org/wet/strafvordering/artikel/2.5.33/lid/1/sublid/d> ;
    dcterms:identifier "artikel-2.5.33-lid-1" .

<http://example.org/wet/strafvordering/artikel/2.5.43> a :Article ;
    rdfs:label "Artikel 2.5.43 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.5 BEWARING)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.43/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.5.43/lid/2> ;
    dcterms:identifier "artikel-2.5.43" .

<http://example.org/wet/strafvordering/artikel/2.5.45/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.6 GEVANGENHOUDING EN GEVANGENNEMING; Artikel Artikel 2.5.45)"@nl ;
    dcterms:description "Wanneer het bevel is gegeven op de terechtzitting, dan wel binnen de krachtens het eerste lid bepaalde termijn het onderzoek op de terecht- zitting is aangevangen, blijft het bevel van kracht totdat twee maanden na de dag van het eindvonnis zijn verstreken."@nl ;
    dcterms:identifier "artikel-2.5.45-lid-2" .

<http://example.org/wet/strafvordering/artikel/2.5.46> a :Article ;
    rdfs:label "Artikel 2.5.46 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.6 GEVANGENHOUDING EN GEVANGENNEMING)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.46/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.5.46/lid/2> ;
    dcterms:identifier "artikel-2.5.46" .

<http://example.org/wet/strafvordering/artikel/2.5.49/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.7 BESLISSINGEN OVER VOORLOPIGE HECHTENIS IN HET EINDVONNIS; Artikel Artikel 2.5.49)"@nl ;
    dcterms:description "De rechtbank heft het bevel tot voorlopige hechtenis op indien zij voor het feit waarvoor voorlopige hechtenis is bevolen, aan de verdachte geen onvoorwaardelijke of gedeeltelijk onvoorwaardelijke vrijheidsstraf oplegt van langere duur dan de al door hem in voorlopige hechtenis doorgebrachte tijd, en evenmin een maatregel oplegt die vrijheidsbe- neming meebrengt."@nl ;
    dcterms:identifier "artikel-2.5.49-lid-1" .

<http://example.org/wet/strafvordering/artikel/2.5.49/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.7 BESLISSINGEN OVER VOORLOPIGE HECHTENIS IN HET EINDVONNIS; Artikel Artikel 2.5.49)"@nl ;
    dcterms:description "Indien de rechtbank voor het feit waarvoor de voorlopige hechtenis is bevolen, een vrijheidsstraf oplegt waarvan het onvoorwaardelijke deel de duur van de al ondergane voorlopige hechtenis met minder dan twee maanden overtreft en geen maatregel oplegt die vrijheidsbeneming meebrengt, heft zij het bevel tot voorlopige hechtenis op met ingang van het tijdstip waarop de duur van deze hechtenis gelijk wordt aan die van de straf."@nl ;
    dcterms:identifier "artikel-2.5.49-lid-2" .

<http://example.org/wet/strafvordering/artikel/2.5.50> a :Article ;
    rdfs:label "Artikel 2.5.50 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.7 BESLISSINGEN OVER VOORLOPIGE HECHTENIS IN HET EINDVONNIS)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.50/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.5.50/lid/2> ;
    dcterms:identifier "artikel-2.5.50" .

<http://example.org/wet/strafvordering/artikel/2.5.52> a :Article ;
    rdfs:label "Artikel 2.5.52 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.8 VOORLOPIGE HECHTENIS NA HOGER BEROEP TEGEN HET EINDVONNIS)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.52/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.5.52/lid/2> ;
    dcterms:identifier "artikel-2.5.52" .

<http://example.org/wet/strafvordering/artikel/2.5.53/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.8 VOORLOPIGE HECHTENIS NA HOGER BEROEP TEGEN HET EINDVONNIS; Artikel Artikel 2.5.53)"@nl ;
    dcterms:description "Indien de verlenging van het bevel op grond van het tweede lid is uitgesloten en door de rechtbank een gedeeltelijk voorwaardelijke vrijheidsstraf als bedoeld in artikel 2.5.49, derde lid, is opgelegd waaraan bijzondere voorwaarden zijn verbonden dan wel een vrijheidsstraf als bedoeld in artikel 2.5.49, derde lid, is opgelegd, in combinatie met de maatregel van terbeschikkingstelling, waaraan bijzondere voorwaarden als bedoeld in artikel 38, eerste lid, van het Wetboek van Strafrecht zijn verbonden, of de maatregel van plaatsing in een inrichting voor jeugdigen, waaraan bijzondere voorwaarden als bedoeld in artikel 77z, tweede lid, van het Wetboek van Strafrecht zijn verbonden kan het gerechtshof de tenuitvoerlegging van het bevel schorsen met ingang van het tijdstip waarop de in voorlopige hechtenis doorgebrachte tijd gelijk wordt aan de duur van het onvoorwaardelijke deel van de vrijheidsstraf. In dat geval blijft het bevel van kracht en worden aan de schorsing van de voorlopige hechtenis geen andere voorwaarden verbonden dan de aan de voorwaardelijke vrijheidsstraf dan wel de maatregel van ter beschikking stelling of de maatregel van plaatsing in een inrichting voor jeugdigen verbonden voorwaarden."@nl ;
    dcterms:identifier "artikel-2.5.53-lid-3" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.5.49/lid/3>,
        <http://example.org/wet/wetboek-van-strafrecht/artikel/2.5.49>,
        <http://example.org/wet/wetboek-van-strafrecht/artikel/38>,
        <http://example.org/wet/wetboek-van-strafrecht/artikel/77> .

<http://example.org/wet/strafvordering/artikel/2.5.55/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.8 VOORLOPIGE HECHTENIS NA HOGER BEROEP TEGEN HET EINDVONNIS; Artikel Artikel 2.5.55)"@nl ;
    dcterms:description "Het gerechtshof heft het bevel tot voorlopige hechtenis op met ingang van het tijdstip waarop de duur van de ondergane voorlopige hechtenis gelijk wordt aan de duur van het onvoorwaardelijk deel van de opgelegde gevangenisstraf, tenzij een maatregel die vrijheidsbeneming meebrengt, is opgelegd."@nl ;
    dcterms:identifier "artikel-2.5.55-lid-2" .

<http://example.org/wet/strafvordering/artikel/2.5.57/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 5.4 VOORLOPIGE HECHTENIS; Afdeling 5.4.8 VOORLOPIGE HECHTENIS NA HOGER BEROEP TEGEN HET EINDVONNIS; Artikel Artikel 2.5.57)"@nl ;
    dcterms:description "Het gerechtshof kan de schorsing van de voorlopige hechtenis op grond van artikel 2.5.49, derde lid, of artikel 2.5.53, derde lid, alleen opheffen indien een daaraan verbonden voorwaarde niet is nageleefd, tenzij het gerechtshof tevens het bevel tot voorlopige hechtenis opheft. Het niet naleven van de voorwaarde vormt een zelfstandige grond voor de tenuitvoerlegging van het bevel tot voorlopige hechtenis."@nl ;
    dcterms:identifier "artikel-2.5.57-lid-1" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.5.49/lid/3>,
        <http://example.org/wet/strafvordering/artikel/2.5.53/lid/3> .

<http://example.org/wet/strafvordering/artikel/2.5.58> a :Article ;
    rdfs:label "Artikel 2.5.58 (Titel 5.5 RECHTSMIDDELEN; Afdeling 5.5.1 BEZWAAR TEGEN HET OPLEGGEN VAN MAATREGELEN)"@nl ;
    dcterms:description "De verdachte kan tegen het bevel tot het opleggen van de maatregelen, bedoeld in artikel 2.5.16, eerste lid, onderdeel a, een bezwaarschrift indienen bij de rechter-commissaris. In geval van voorlopige hechtenis wordt het bezwaarschrift ingediend bij de rechter-commissaris die de bewaring heeft bevolen dan wel de rechtbank die de gevangenhouding of gevangenneming heeft bevolen."@nl ;
    dcterms:identifier "artikel-2.5.58" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.5.16/lid/1> .

<http://example.org/wet/strafvordering/artikel/2.5.59> a :Article ;
    rdfs:label "Artikel 2.5.59 (Titel 5.5 RECHTSMIDDELEN; Afdeling 5.5.2 BEROEP TEGEN DE DIRECTE INVRIJHEIDSTELLING VAN DE VERDACHTE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.5.59/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.5.59/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.5.59/lid/3> ;
    dcterms:identifier "artikel-2.5.59" .

<http://example.org/wet/strafvordering/artikel/2.5.9/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 5.3 OPHOUDEN VOOR ONDERZOEK EN INVERZEKERING- STELLING; Afdeling 5.3.1 OPHOUDEN VOOR ONDERZOEK; Artikel Artikel 2.5.9)"@nl ;
    dcterms:description "De verdachte van een misdrijf waarop naar de wettelijke omschrijving gevangenisstraf van vier jaar of meer is gesteld en de verdachte van wie geen vaste woon- of verblijfplaats in Nederland kan worden vastgesteld en die wordt verdacht van een misdrijf waarop naar de wettelijke omschrijving gevangenisstraf is gesteld, kunnen ten hoogste negen uur worden opgehouden voor onderzoek; in de overige gevallen kan de verdachte ten hoogste zes uur worden opgehouden voor onderzoek. De tijd tussen middernacht en negen uur 's morgens wordt voor de berekening van deze termijnen niet meegerekend."@nl ;
    dcterms:identifier "artikel-2.5.9-lid-2" .

<http://example.org/wet/strafvordering/artikel/2.6.12/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 6.5 OVERIGE ONDERZOEKEN MET BETREKKING TOT HET LICHAAM; Afdeling 6.5.3 HET MAKEN VAN BEELDOPNAMEN, HET OPMETEN VAN LICHAAMSMATEN EN HET NEMEN VAN LICHAAMSAFDRUKKEN, -HAAR EN -MATERIAAL; Artikel Artikel 2.6.12)"@nl ;
    dcterms:description "In geval van verdenking van een misdrijf waarop naar de wettelijke omschrijving gevangenisstraf van vier jaar of meer is gesteld, kan de officier van justitie of de hulpofficier van justitie bevelen dat de opsporingsambtenaar ten behoeve van een vergelijkend onderzoek:"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.6.12/lid/1/sublid/a>,
        <http://example.org/wet/strafvordering/artikel/2.6.12/lid/1/sublid/b>,
        <http://example.org/wet/strafvordering/artikel/2.6.12/lid/1/sublid/c> ;
    dcterms:identifier "artikel-2.6.12-lid-1" .

<http://example.org/wet/strafvordering/artikel/2.6.17> a :Article ;
    rdfs:label "Artikel 2.6.17 (Titel 6.5 OVERIGE ONDERZOEKEN MET BETREKKING TOT HET LICHAAM; Afdeling 6.5.6 DNA-ONDERZOEK)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.6.17/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.6.17/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.6.17/lid/3>,
        <http://example.org/wet/strafvordering/artikel/2.6.17/lid/4>,
        <http://example.org/wet/strafvordering/artikel/2.6.17/lid/5>,
        <http://example.org/wet/strafvordering/artikel/2.6.17/lid/6>,
        <http://example.org/wet/strafvordering/artikel/2.6.17/lid/7> ;
    dcterms:identifier "artikel-2.6.17" .

<http://example.org/wet/strafvordering/artikel/2.6.20> a :Article ;
    rdfs:label "Artikel 2.6.20 (Titel 6.5 OVERIGE ONDERZOEKEN MET BETREKKING TOT HET LICHAAM; Afdeling 6.5.7 ONDERZOEK NAAR EEN ERNSTIGE BESMETTELIJKE ZIEKTE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.6.20/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.6.20/lid/2>,
        <http://example.org/wet/strafvordering/artikel/2.6.20/lid/3>,
        <http://example.org/wet/strafvordering/artikel/2.6.20/lid/4>,
        <http://example.org/wet/strafvordering/artikel/2.6.20/lid/5>,
        <http://example.org/wet/strafvordering/artikel/2.6.20/lid/6> ;
    dcterms:identifier "artikel-2.6.20" .

<http://example.org/wet/strafvordering/artikel/2.6.20/lid/4> a :Paragraph ;
    rdfs:label "Lid 4 (Titel 6.5 OVERIGE ONDERZOEKEN MET BETREKKING TOT HET LICHAAM; Afdeling 6.5.7 ONDERZOEK NAAR EEN ERNSTIGE BESMETTELIJKE ZIEKTE; Artikel Artikel 2.6.20)"@nl ;
    dcterms:description "Het bevel of de tenuitvoerlegging of de verdere tenuitvoerlegging daarvan kan achterwege blijven indien de verdachte zich tegen de afname van zijn celmateriaal verzet of vermist is. In dat geval kan het onderzoek worden verricht aan veiliggestelde sporen of het celmateriaal dat op een inbeslaggenomen voorwerp aanwezig is of op andere wijze verkregen is, mits voldoende zekerheid bestaat dat dat celmateriaal van de verdachte afkomstig is."@nl ;
    dcterms:identifier "artikel-2.6.20-lid-4" .

<http://example.org/wet/strafvordering/artikel/2.6.23/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 6.5 OVERIGE ONDERZOEKEN MET BETREKKING TOT HET LICHAAM; Afdeling 6.5.7 ONDERZOEK NAAR EEN ERNSTIGE BESMETTELIJKE ZIEKTE; Artikel Artikel 2.6.23)"@nl ;
    dcterms:description "Het slachtoffer van een misdrijf als bedoeld in artikel 2.6.20, eerste lid, kan de officier van justitie verzoeken een onderzoek als bedoeld in deze afdeling te bevelen."@nl ;
    dcterms:identifier "artikel-2.6.23-lid-1" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/2.6.20/lid/1> .

<http://example.org/wet/strafvordering/artikel/2.6.24> a :Article ;
    rdfs:label "Artikel 2.6.24 (Titel 6.6 ONDERZOEK MET BETREKKING TOT HET LICHAAM VAN EEN OVERLEDEN VERDACHTE OF SLACHTOFFER)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/2.6.24/lid/1>,
        <http://example.org/wet/strafvordering/artikel/2.6.24/lid/2>,
