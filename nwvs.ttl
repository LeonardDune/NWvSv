        <http://example.org/wet/strafvordering/artikel/5.1.2/lid/3> ;
    dcterms:identifier "artikel-5.1.2" .

<http://example.org/wet/strafvordering/artikel/5.1.2/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Boek 5 RECHTSMIDDELEN; Hoofdstuk 1 ALGEMENE BEPALINGEN; Artikel Artikel 5.1.2)"@nl ;
    dcterms:description "Er zijn gewone en buitengewone rechtsmiddelen."@nl ;
    dcterms:identifier "artikel-5.1.2-lid-1" .

<http://example.org/wet/strafvordering/artikel/5.1.2/lid/2> a :Paragraph ;
