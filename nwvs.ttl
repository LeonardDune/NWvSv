    rdfs:label "Artikel 1.8.12 (Titel 8.2 RECHTSMIDDELEN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.8.12/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.8.12/lid/2> ;
