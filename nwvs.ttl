<http://example.org/wet/strafvordering/artikel/1.9.16/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 9.2 BETEKENING; Artikel Artikel 1.9.16)"@nl ;
    dcterms:description "Van iedere betekening door uitreiking wordt vastgelegd:"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.9.16/lid/1/sublid/a>,
        <http://example.org/wet/strafvordering/artikel/1.9.16/lid/1/sublid/b>,
