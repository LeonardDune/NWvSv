<http://example.org/wet/strafvordering/artikel/5.8.18> a :Article ;
    rdfs:label "Artikel 5.8.18 (Titel 8.1 HERZIENING TEN VOORDELE VAN DE GEWEZEN VERDACHTE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/5.8.18/lid/1>,
        <http://example.org/wet/strafvordering/artikel/5.8.18/lid/2>,
        <http://example.org/wet/strafvordering/artikel/5.8.18/lid/3> ;
    dcterms:identifier "artikel-5.8.18" .

<http://example.org/wet/strafvordering/artikel/5.8.27> a :Article ;
    rdfs:label "Artikel 5.8.27 (Titel 8.2 HERZIENING TEN NADELE VAN DE GEWEZEN VERDACHTE)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/5.8.27/lid/1>,
        <http://example.org/wet/strafvordering/artikel/5.8.27/lid/2>,
