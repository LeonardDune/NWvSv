<http://example.org/wet/strafvordering/artikel/4.4.11> a :Article ;
    rdfs:label "Artikel 4.4.11 (Titel 4.2 DE VORDERING TOT TENUITVOERLEGGING)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/4.4.11/lid/1>,
        <http://example.org/wet/strafvordering/artikel/4.4.11/lid/2> ;
