    dcterms:references <http://example.org/wet/strafvordering/artikel/1.8.5/lid/4> .

<http://example.org/wet/strafvordering/artikel/1.8.12/lid/2> a :Paragraph ;
    rdfs:label "Lid 2 (Titel 8.2 RECHTSMIDDELEN; Artikel Artikel 1.8.12)"@nl ;
    dcterms:description "De termijn voor het instellen van beroep is twee weken na de dagtekening van de beslissing. De rechtbank beslist zo spoedig mogelijk."@nl ;
    dcterms:identifier "artikel-1.8.12-lid-2" .

<http://example.org/wet/strafvordering/artikel/1.8.13> a :Article ;
    rdfs:label "Artikel 1.8.13 (Titel 8.2 RECHTSMIDDELEN)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.8.13/lid/1>,
        <http://example.org/wet/strafvordering/artikel/1.8.13/lid/2> ;
    dcterms:identifier "artikel-1.8.13" .

<http://example.org/wet/strafvordering/artikel/1.8.13/lid/1> a :Paragraph ;
    rdfs:label "Lid 1 (Titel 8.2 RECHTSMIDDELEN; Artikel Artikel 1.8.13)"@nl ;
    dcterms:description "De verdachte kan een bezwaarschrift indienen bij de rechter- commissaris tegen:"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.8.13/lid/1/sublid/a>,
        <http://example.org/wet/strafvordering/artikel/1.8.13/lid/1/sublid/b>,
        <http://example.org/wet/strafvordering/artikel/1.8.13/lid/1/sublid/c>,
        <http://example.org/wet/strafvordering/artikel/1.8.13/lid/1/sublid/d> ;
    dcterms:identifier "artikel-1.8.13-lid-1" .

<http://example.org/wet/strafvordering/artikel/1.8.13/lid/1/sublid/a> a :Subparagraph ;
    rdfs:label "Sublid a (Titel 8.2 RECHTSMIDDELEN; Artikel Artikel 1.8.13)"@nl ;
    dcterms:description "de kennisgeving, bedoeld in artikel 1.8.4, eerste lid, dat zijn verzoek om het voegen van stukken is afgewezen;"@nl ;
    dcterms:identifier "artikel-1.8.13-lid-1-sublid-a" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/1.8.4/lid/1> .

<http://example.org/wet/strafvordering/artikel/1.8.13/lid/1/sublid/b> a :Subparagraph ;
    rdfs:label "Sublid b (Titel 8.2 RECHTSMIDDELEN; Artikel Artikel 1.8.13)"@nl ;
    dcterms:description "de kennisgeving, bedoeld in artikel 1.8.6, tweede lid, dat de proces- stukken niet volledig zijn;"@nl ;
    dcterms:identifier "artikel-1.8.13-lid-1-sublid-b" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/1.8.6/lid/2> .

<http://example.org/wet/strafvordering/artikel/1.8.13/lid/1/sublid/c> a :Subparagraph ;
    rdfs:label "Sublid c (Titel 8.2 RECHTSMIDDELEN; Artikel Artikel 1.8.13)"@nl ;
    dcterms:description "de kennisgeving, bedoeld in artikel 1.8.8, derde lid, van de officier van justitie dat hem van bepaalde stukken of gedeelten daarvan geen kopie wordt verstrekt;"@nl ;
    dcterms:identifier "artikel-1.8.13-lid-1-sublid-c" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/1.8.8/lid/3> .

<http://example.org/wet/strafvordering/artikel/1.8.13/lid/1/sublid/d> a :Subparagraph ;
