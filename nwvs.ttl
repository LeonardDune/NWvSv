    rdfs:label "Lid 1 (Titel 8.2 RECHTSMIDDELEN; Artikel Artikel 1.8.12)"@nl ;
    dcterms:description "De officier van justitie kan beroep instellen tegen de afwijzing door de rechter-commissaris van een vordering op grond van:"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/1.8.12/lid/1/sublid/a>,
        <http://example.org/wet/strafvordering/artikel/1.8.12/lid/1/sublid/b> ;
    dcterms:identifier "artikel-1.8.12-lid-1" .

<http://example.org/wet/strafvordering/artikel/1.8.12/lid/1/sublid/a> a :Subparagraph ;
    rdfs:label "Sublid a (Titel 8.2 RECHTSMIDDELEN; Artikel Artikel 1.8.12)"@nl ;
    dcterms:description "artikel 1.8.3, eerste lid;"@nl ;
    dcterms:identifier "artikel-1.8.12-lid-1-sublid-a" ;
    dcterms:references <http://example.org/wet/strafvordering/artikel/1.8.3/lid/1> .

<http://example.org/wet/strafvordering/artikel/1.8.12/lid/1/sublid/b> a :Subparagraph ;
    rdfs:label "Sublid b (Titel 8.2 RECHTSMIDDELEN; Artikel Artikel 1.8.12)"@nl ;
