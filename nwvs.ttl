        <http://example.org/wet/strafvordering/artikel/4.2.62/lid/4> ;
    dcterms:identifier "artikel-4.2.62" .

<http://example.org/wet/strafvordering/artikel/4.2.69> a :Article ;
    rdfs:label "Artikel 4.2.69 (Titel 2.5 DE VERSLAGLEGGING VAN HET ONDERZOEK OP DE TERECHTZITTING)"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/4.2.69/lid/1>,
        <http://example.org/wet/strafvordering/artikel/4.2.69/lid/2>,
