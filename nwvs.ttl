    rdfs:label "Lid 1 (Titel 1.2 OPROEPING VOOR DE TERECHTZITTING EN INTREKKING VAN DE PROCESINLEIDING; Artikel Artikel 4.1.11)"@nl ;
    dcterms:description "De officier van justitie roept voor de terechtzitting op:"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/4.1.11/lid/1/sublid/a>,
        <http://example.org/wet/strafvordering/artikel/4.1.11/lid/1/sublid/b>,
        <http://example.org/wet/strafvordering/artikel/4.1.11/lid/1/sublid/c> ;
    dcterms:identifier "artikel-4.1.11-lid-1" .

<http://example.org/wet/strafvordering/artikel/4.1.11/lid/3> a :Paragraph ;
    rdfs:label "Lid 3 (Titel 1.2 OPROEPING VOOR DE TERECHTZITTING EN INTREKKING VAN DE PROCESINLEIDING; Artikel Artikel 4.1.11)"@nl ;
    dcterms:description "De officier van justitie roept voorts voor de terechtzitting op:"@nl ;
    dcterms:hasPart <http://example.org/wet/strafvordering/artikel/4.1.11/lid/3/sublid/a>,
        <http://example.org/wet/strafvordering/artikel/4.1.11/lid/3/sublid/b> ;
    dcterms:identifier "artikel-4.1.11-lid-3" .

